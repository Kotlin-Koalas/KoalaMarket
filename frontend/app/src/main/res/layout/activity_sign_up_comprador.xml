<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpComprador"
    android:background="@drawable/fondo_hojitas">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.08" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/basic_white_rectangle">
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guidelineCL"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.07" />
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@id/guidelineCL"
            app:layout_constraintStart_toStartOf="parent">
            <ScrollView
                android:id="@+id/scrollView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="50dp"
                android:layout_marginEnd="50dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textViewTitle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#FFFFFF"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Fill the information bellow"
                        android:textSize="17sp" />
                    <TextView
                        android:id="@+id/textViewID1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="30dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="ID de Usuario:"
                        android:textSize="17sp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutID"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewID1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/icono_persona_borde" />

                        <ImageView
                            android:id="@+id/imageViewID2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextID"
                            app:layout_constraintStart_toEndOf="@+id/imageViewID1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextID"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="Ejemplo"
                            android:inputType="text"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewID2"
                            tools:layout_editor_absoluteY="341dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <TextView
                        android:id="@+id/textViewName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Nombre:"
                        android:textSize="17sp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutName"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewName1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/icono_persona_borde" />

                        <ImageView
                            android:id="@+id/imageViewName2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextName"
                            app:layout_constraintStart_toEndOf="@+id/imageViewName1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextName"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="Ejemplo"
                            android:inputType="text"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewName2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewSurname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Apellidos:"
                        android:textSize="17sp"/>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutSurname"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewSurname1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/icono_persona_borde" />

                        <ImageView
                            android:id="@+id/imageViewSurname2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextSurname"
                            app:layout_constraintStart_toEndOf="@+id/imageViewSurname1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextSurname"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="Ejemplo"
                            android:inputType="text"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewSurname2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewEmail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Email:"
                        android:textSize="17sp"/>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutEmail"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewEmail1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/email_icon" />

                        <ImageView
                            android:id="@+id/imageViewEmail2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextEmail"
                            app:layout_constraintStart_toEndOf="@+id/imageViewEmail1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextEmail"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="ejemplo@ejemplo.com"
                            android:inputType="textEmailAddress"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewEmail2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewPassword"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Contraseña:"
                        android:textSize="17sp"/>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutPassword"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewPassword1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/password_icon" />

                        <ImageView
                            android:id="@+id/imageViewPassword2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextPassword"
                            app:layout_constraintStart_toEndOf="@+id/imageViewPassword1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextPassword"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="Ejemplo123"
                            android:inputType="textPassword"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewPassword2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <TextView
                        android:id="@+id/textViewRPassword"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Repetir Contraseña:"
                        android:textSize="17sp"/>

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutRPassword"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                    <ImageView
                        android:id="@+id/imageViewRPassword1"
                        android:layout_width="27dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:layout_marginLeft="8dp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:srcCompat="@drawable/password_icon" />

                    <ImageView
                        android:id="@+id/imageViewRPassword2"
                        android:layout_width="1.5dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="8dp"
                        android:background="#bababa"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/editTextRPassword"
                        app:layout_constraintStart_toEndOf="@+id/imageViewRPassword1"
                        app:layout_constraintTop_toTopOf="parent" />

                    <EditText
                        android:id="@+id/editTextRPassword"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="right"
                        android:layout_marginLeft="8dp"
                        android:background="@android:color/transparent"
                        android:ems="8"
                        android:fontFamily="@font/karla"
                        android:gravity="center_vertical"
                        android:hint="Ejemplo123"
                        android:inputType="textPassword"
                        android:textColor="@color/black"
                        android:textColorHint="#808080"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/imageViewRPassword2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewDNI"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="DNI/NIE:"
                        android:textSize="17sp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutDNI"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewDNI1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/icono_persona_borde" />

                        <ImageView
                            android:id="@+id/imageViewDNI2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextDNI"
                            app:layout_constraintStart_toEndOf="@+id/imageViewDNI1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextDNI"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="12345678E"
                            android:inputType="text"
                            android:maxLength="9"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewDNI2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewAddress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Dirección de Entrega:"
                        android:textSize="17sp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutAddress"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewAddress1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/location_icon" />

                        <ImageView
                            android:id="@+id/imageViewAddress2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextAddress"
                            app:layout_constraintStart_toEndOf="@+id/imageViewAddress1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextAddress"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="Ejemplo, 12"
                            android:inputType="text"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewAddress2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewFAddress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Dirección de Facturación:"
                        android:textSize="17sp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutFAddress"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewFAddress1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/location_icon" />

                        <ImageView
                            android:id="@+id/imageViewFAddress2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/editTextFAddress"
                            app:layout_constraintStart_toEndOf="@+id/imageViewFAddress1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <EditText
                            android:id="@+id/editTextFAddress"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@android:color/transparent"
                            android:ems="8"
                            android:fontFamily="@font/karla"
                            android:gravity="center_vertical"
                            android:hint="Ejemplo, 12"
                            android:inputType="text"
                            android:textColor="@color/black"
                            android:textColorHint="#808080"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewFAddress2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/textViewSPM"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="7dp"
                        android:layout_marginBottom="3dp"
                        android:layout_marginTop="10dp"
                        android:background="#ffffff"
                        android:textColor="@color/black"
                        android:fontFamily="@font/karla"
                        android:text="Seleccionar método de pago:"
                        android:textSize="17sp" />

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/frameLayoutSPM"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:background="@drawable/basic_grey_shape"
                        android:padding="2dp">

                        <ImageView
                            android:id="@+id/imageViewSPM1"
                            android:layout_width="27dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="center"
                            android:layout_marginLeft="8dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:srcCompat="@drawable/icono_dinero" />

                        <ImageView
                            android:id="@+id/imageViewSPM2"
                            android:layout_width="1.5dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="8dp"
                            android:background="#bababa"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/spinnerSPM"
                            app:layout_constraintStart_toEndOf="@+id/imageViewSPM1"
                            app:layout_constraintTop_toTopOf="parent" />

                        <Spinner
                            android:id="@+id/spinnerSPM"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_gravity="right"
                            android:layout_marginLeft="8dp"
                            android:background="@drawable/spinner_background"
                            android:gravity="bottom"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/imageViewSPM2"
                            tools:layout_editor_absoluteY="2dp"
                            />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </LinearLayout>
            </ScrollView>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>